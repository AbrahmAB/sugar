import gettext

import gtk

from jarabe import config
from jarabe import intro


gettext.bindtextdomain('sugar', config.locale_path)
gettext.bindtextdomain('sugar-toolkit', config.locale_path)
gettext.textdomain('sugar')
intro.check_profile()
